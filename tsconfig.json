{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "typeRoots": [
      "./node_modules/@types",
      "./declarations.d.ts"
    ],
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "module": "esnext"
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "app/[missing.tsx]",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
