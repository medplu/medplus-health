{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "typeRoots": [
      "./node_modules/@types",
      "./declarations.d.ts"
    ],
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "app/[missing.tsx]"
  ]
}
